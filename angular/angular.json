{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "design": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/design",
      "sourceRoot": "projects/design/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "projects/design/webpack.config.js"
            },
            "outputPath": "dist/design",
            "index": "projects/design/src/index.html",
            "main": "projects/design/src/main.ts",
            "polyfills": "projects/design/src/polyfills.ts",
            "tsConfig": "projects/design/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["projects/design/src/favicon.ico", "projects/design/src/assets"],
            "styles": ["projects/design/src/styles.scss", "projects/design/src/icons.font.js"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/design/src/environments/environment.ts",
                  "with": "projects/design/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "design:build:production"
            },
            "development": {
              "browserTarget": "design:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "design:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/design/src/test.ts",
            "polyfills": "projects/design/src/polyfills.ts",
            "tsConfig": "projects/design/tsconfig.spec.json",
            "karmaConfig": "projects/design/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["projects/design/src/favicon.ico", "projects/design/src/assets"],
            "styles": ["projects/design/src/styles.scss"],
            "scripts": []
          }
        }
      }
    },
    "shared": {
      "projectType": "library",
      "root": "projects/shared",
      "sourceRoot": "projects/shared/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/shared/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/shared/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/shared/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/shared/src/test.ts",
            "tsConfig": "projects/shared/tsconfig.spec.json",
            "karmaConfig": "projects/shared/karma.conf.js"
          }
        }
      }
    },
    "auth": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/auth",
      "sourceRoot": "projects/auth/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "projects/auth/webpack.config.js"
            },
            "outputPath": "dist/auth",
            "index": "projects/auth/src/index.html",
            "main": "projects/auth/src/main.ts",
            "polyfills": "projects/auth/src/polyfills.ts",
            "tsConfig": "projects/auth/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["projects/auth/src/favicon.ico", "projects/auth/src/assets"],
            "styles": ["projects/auth/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/auth/src/environments/environment.ts",
                  "with": "projects/auth/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "auth:build:production"
            },
            "development": {
              "browserTarget": "auth:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "auth:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/auth/src/test.ts",
            "polyfills": "projects/auth/src/polyfills.ts",
            "tsConfig": "projects/auth/tsconfig.spec.json",
            "karmaConfig": "projects/auth/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["projects/auth/src/favicon.ico", "projects/auth/src/assets"],
            "styles": ["projects/auth/src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "design"
}
