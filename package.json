{
  "name": "peak-server",
  "version": "2.0.0-alpha.10",
  "private": true,
  "scripts": {
    "webpack-dev": "cross-env NODE_ENV=development webpack --mode development",
    "webpack-prod": "cross-env NODE_ENV=production webpack --mode production",
    "build": "npm run build:prod",
    "build:dev": "webpack-dev-server",
    "build:prod": "npm run build:clean && npm run webpack-prod && npm run build:clean",
    "build:clean": "del-cli 'client/**/*.js' '!client/**/*.font.js' 'client/**/*.js.map' 'client/**/*.ngsummary.json' 'client/**/*.metadata.json' 'client/**/**/*.ngfactory.ts' 'client/**/*.ngstyle.ts' 'client/**/*.shim.ts'",
    "start": "nodemon -V --ignore ./angular --ignore ./client --ignore ./nemsis --ignore ./public  --ignore ./test --ignore ./tmp ./bin/www",
    "test": "mocha --unhandled-rejections=strict --recursive test/unit test/integration",
    "ci": "prettier --check . && npm run lint && npm run test",
    "lint": "eslint .",
    "lint:changed": "eslint $(git diff --name-only; git ls-files -o --exclude-standard)",
    "pp": "prettier --write .",
    "pp:changed": "prettier --write $(git diff --name-only; git ls-files -o --exclude-standard)"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "es2020": true,
      "mocha": true
    },
    "extends": [
      "plugin:mocha/recommended",
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "mocha",
      "prettier"
    ],
    "rules": {
      "mocha/no-mocha-arrows": [
        "off"
      ],
      "no-underscore-dangle": [
        "off"
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-restricted-syntax": [
        "error",
        {
          "selector": "ForInStatement",
          "message": "for..in loops iterate over the entire prototype chain, which is virtually never what you want. Use Object.{keys,values,entries}, and iterate over the resulting array."
        },
        {
          "selector": "LabeledStatement",
          "message": "Labels are a form of GOTO; using them makes code confusing and hard to maintain and understand."
        },
        {
          "selector": "WithStatement",
          "message": "`with` is disallowed in strict mode because it makes code impossible to predict and optimize."
        }
      ]
    },
    "ignorePatterns": [
      "dist",
      "node_modules",
      "*.conf.js"
    ],
    "overrides": [
      {
        "files": [
          "public/scripts/**/*.js"
        ],
        "env": {
          "browser": true,
          "jquery": true
        }
      },
      {
        "files": [
          "migrations/*.js"
        ],
        "rules": {
          "no-unused-vars": [
            "error",
            {
              "argsIgnorePattern": "queryInterface|Sequelize"
            }
          ]
        }
      }
    ]
  },
  "mocha": {
    "timeout": 120000
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 140,
    "singleQuote": true
  },
