<%- contentFor('title') %><%= __('admin.title') %>
<%- contentFor('head') %>
<base href="/admin">
<script type="text/javascript">
  window.env = {};
  window.env.BASE_HOST = '<%= process.env.BASE_HOST %>';
  window.env.MARKETING_ENABLED = '<%= process.env.MARKETING_ENABLED %>';
</script>
<%- contentFor('body') %>
<app-root><%= __('common.pleaseWait') %></app-root>
<% for (let asset of Object.keys(webpackStats.assets)) { %>
  <% if (asset.endsWith('.js')) { %>
    <script type="application/javascript" src="<%= webpackStats.assets[asset].publicPath %>" defer></script>
  <% } %>
<% } %>
