									</nemsisTacDoc>
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ePayment.06" type="PersonLastName" id="ePayment.LastNameofIndividualSigningPhysicianCertificationStatement" minOccurs="0">
							<xs:annotation>
