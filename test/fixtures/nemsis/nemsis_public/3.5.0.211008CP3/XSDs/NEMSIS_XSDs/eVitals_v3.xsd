							<xs:annotation>
								<xs:documentation>
									<nemsisTacDoc>
										<number>eVitals.13</number>
										<name>Pulse Rhythm</name>
										<national>No</national>
										<state>No</state>
										<definition>The clinical rhythm of the patient's pulse.</definition>
										<v2Number>E14_10</v2Number>
										<usage>Optional</usage>
									</nemsisTacDoc>
