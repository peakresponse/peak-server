						<xs:element name="eVitals.15" id="eVitals.RespiratoryEffort" minOccurs="0">
							<xs:annotation>
								<xs:documentation>
									<nemsisTacDoc>
										<number>eVitals.15</number>
										<name>Respiratory Effort</name>
										<national>No</national>
										<state>No</state>
										<definition>The patient's respiratory effort.</definition>
