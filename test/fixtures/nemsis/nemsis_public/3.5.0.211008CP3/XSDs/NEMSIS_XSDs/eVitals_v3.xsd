						</xs:element>
						<xs:element name="eVitals.14" nillable="true" id="eVitals.RespiratoryRate">
							<xs:annotation>
								<xs:documentation>
									<nemsisTacDoc>
										<number>eVitals.14</number>
										<name>Respiratory Rate</name>
										<national>Yes</national>
										<state>Yes</state>
										<definition>The patient's respiratory rate expressed as a number per minute.</definition>
										<v2Number>E14_11</v2Number>
										<usage>Required</usage>
										<performanceMeasuresGroup>
											<performanceMeasure>Airway</performanceMeasure>
											<performanceMeasure>Cardiac Arrest</performanceMeasure>
											<performanceMeasure>Pediatric</performanceMeasure>
											<performanceMeasure>STEMI</performanceMeasure>
											<performanceMeasure>Stroke</performanceMeasure>
											<performanceMeasure>Trauma</performanceMeasure>
										</performanceMeasuresGroup>
										<PNNil>Yes</PNNil>
									</nemsisTacDoc>
								</xs:documentation>
							</xs:annotation>
							<xs:complexType>
