														<xs:simpleType>
															<xs:union memberTypes="PN.ExamFindingNotPresent PN.Refused PN.UnableToComplete"/>
														</xs:simpleType>
													</xs:attribute>
													<xs:attribute name="NV" use="optional">
														<xs:simpleType>
															<xs:union memberTypes="NV.NotApplicable NV.NotRecorded NV.NotReporting"/>
														</xs:simpleType>
													</xs:attribute>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="eVitals.08" nillable="true" id="eVitals.MethodofBloodPressureMeasurement" minOccurs="0">
										<xs:annotation>
											<xs:documentation>
												<nemsisTacDoc>
													<number>eVitals.08</number>
													<name>Method of Blood Pressure Measurement</name>
													<national>No</national>
													<state>Yes</state>
													<definition>Indication of method of blood pressure measurement.</definition>
													<v2Number>E14_06</v2Number>
													<usage>Recommended</usage>
												</nemsisTacDoc>
											</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="BloodPressureMethod">
													<xs:attribute name="NV" use="optional">
														<xs:simpleType>
															<xs:union memberTypes="NV.NotApplicable NV.NotRecorded NV.NotReporting"/>
														</xs:simpleType>
													</xs:attribute>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="eVitals.09" type="MAP" id="eVitals.MeanArterialPressure" minOccurs="0">
										<xs:annotation>
											<xs:documentation>
												<nemsisTacDoc>
													<number>eVitals.09</number>
													<name>Mean Arterial Pressure</name>
													<national>No</national>
													<state>No</state>
													<definition>The patient's mean arterial pressure.</definition>
													<usage>Optional</usage>
													<v3Changes>Added for additional patient monitoring capability.</v3Changes>
												</nemsisTacDoc>
											</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="eVitals.HeartRateGroup" id="eVitals.HeartRateGroup">
