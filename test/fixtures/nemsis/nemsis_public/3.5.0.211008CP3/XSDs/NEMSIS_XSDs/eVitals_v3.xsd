												<xs:extension base="SBP">
													<xs:attribute name="PN" use="optional">
														<xs:simpleType>
															<xs:union memberTypes="PN.ExamFindingNotPresent PN.Refused PN.UnableToComplete"/>
														</xs:simpleType>
													</xs:attribute>
													<xs:attribute name="NV" use="optional">
														<xs:simpleType>
															<xs:union memberTypes="NV.NotApplicable NV.NotRecorded"/>
														</xs:simpleType>
													</xs:attribute>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="eVitals.07" nillable="true" id="eVitals.DBPDiastolicBloodPressure" minOccurs="0">
										<xs:annotation>
											<xs:documentation>
												<nemsisTacDoc>
													<number>eVitals.07</number>
													<name>DBP (Diastolic Blood Pressure)</name>
													<national>No</national>
													<state>Yes</state>
													<definition>The patient's diastolic blood pressure.</definition>
													<v2Number>E14_05</v2Number>
													<usage>Recommended</usage>
													<comment>Diastolic blood pressure pattern allows for the following values:&lt;br/>1) A number 0 through 500 &lt;br/>2) P&lt;br/>3) p</comment>
													<PNNil>Yes</PNNil>
												</nemsisTacDoc>
											</xs:documentation>
